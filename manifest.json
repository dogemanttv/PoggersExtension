{

    "manifest_version": 2,
    "name": "Poggers",
    "version": "2.0",
  
    "description": "Changes every image to a picture of the pogchamp.",
  
    "icons": {
      "600": "icons/pogchamp600.png",
      "1600": "icons/pogchamp1600.png"
    },
  
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["poggers.js"]
      }
    ],

    "browser_action": {
      "default_popup": "src/HTML/popup.html",
      "browser_style": true
    }
    
  
  }
  